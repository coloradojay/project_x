<h1>New User</h1>

<%= form_for @user do |f| %>
		<%= f.label :user_name %> <%= f.text_field :user_name %>		
		<%= f.label :name %> <%= f.text_field :name %>
		<%= f.label :email %> <%= f.text_field :email %>
		<%= f.label :image %> <%= f.text_field :image %>

	<%= f.submit "Add User!" %>

<% end %>

<%= link_to "Back to the list!", posts_path %>
